<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="org.pltw.examples.collegeapp"
          android:versionCode="1"
          android:versionName="1.0">

          <uses-sdk android:minSkdVersion="8" android:targetSdkVersion="21" />
          <uses-permission android:name="android.permission.CAMERA" />
          <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.pltw.examples.collegeapp.ApplicantActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
        <activity android:name="org.pltw.examples.collegeapp.FamilyMemberActivity"
            android:label="CollegeApp">
        </activity>

        <activity
            android:name=".CameraActivity"
            android:screenOrientation="landscape"
            android:label="@string/app_name"></activity>

    </application>

</manifest>
